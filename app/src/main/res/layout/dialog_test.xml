<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <RelativeLayout
            android:id="@+id/relativeLayout"

            android:paddingBottom="150dp"
            android:layout_marginTop="150dp"
            android:layout_marginRight="15dp"
            android:layout_marginLeft="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/dd"
                android:background="#fff"
                android:layout_width="match_parent"
                android:layout_height="30dp"/>


        <ScrollView
            android:id="@+id/tt"
            android:layout_below="@id/dd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#fff"
                android:scrollbarAlwaysDrawVerticalTrack="true"
                >

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/rp"
                    android:paddingTop="20dp"
                    android:background="#fff"
                    android:orientation="vertical"
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    xmlns:tools="http://schemas.android.com/tools"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <!--    <androidx.appcompat.widget.AppCompatRatingBar-->
                    <!--    android:layout_gravity="center"-->
                    <!--        android:layout_marginTop="50dp"-->
                    <!--        android:id="@+id/emp_total_rate"-->
                    <!--        android:layout_width="wrap_content"-->
                    <!--        android:layout_height="wrap_content"-->
                    <!--        android:numStars="5"-->
                    <!--        android:stepSize="0.1"-->
                    <!--        tools:layout_editor_absoluteX="85dp"-->
                    <!--        tools:layout_editor_absoluteY="44dp" />-->

                    <!--    <Button-->
                    <!--        android:id="@+id/fr_to_rate_emp_btn"-->
                    <!--        android:layout_width="wrap_content"-->
                    <!--        android:layout_height="wrap_content"-->
                    <!--        android:background="#fff"-->
                    <!--        android:text="Rate"-->
                    <!--        android:layout_marginTop="30dp"-->
                    <!--        android:layout_gravity="center"-->
                    <!--        android:textColor="@color/colorPrimary"-->
                    <!--        tools:layout_editor_absoluteX="161dp"-->
                    <!--        tools:layout_editor_absoluteY="127dp" />-->


                    <!--    <androidx.viewpager.widget.ViewPager-->
                    <!--        android:id="@+id/fr_vp"-->
                    <!--        android:layout_width="333dp"-->
                    <!--        android:layout_height="212dp"-->
                    <!--        android:layout_gravity="center"-->
                    <!--        android:layout_marginTop="30dp"-->
                    <!--        android:background="#64A9CD"></androidx.viewpager.widget.ViewPager>-->

                    <TextView
                        android:id="@+id/rq_one"
                        android:text="QUESTION 1"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="20dp"
                        app:layout_constraintTop_toTopOf="@id/rp"
                        app:layout_constraintStart_toStartOf="@id/rp"
                        app:layout_constraintEnd_toEndOf="@id/rp"
                        android:layout_width="match_parent"
                        android:gravity="center"
                        android:layout_height="wrap_content"/>


                    <RatingBar
                        android:id="@+id/rbq_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        app:layout_constraintTop_toBottomOf="@id/rq_one"
                        app:layout_constraintStart_toStartOf="@id/rp"
                        app:layout_constraintEnd_toEndOf="@id/rp"
                        android:numStars="5"
                        android:scaleX=".7"
                        android:scaleY=".7"
                        android:stepSize="1"
                        android:theme="@style/RatingBar"
                        />


                    <TextView
                        android:id="@+id/rq_two"
                        android:text="QUESTION 2"
                        android:gravity="center"

                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="20dp"
                        app:layout_constraintTop_toBottomOf="@id/rbq_one"
                        app:layout_constraintStart_toStartOf="@id/rp"
                        app:layout_constraintEnd_toEndOf="@id/rp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>

                    <RatingBar
                        android:id="@+id/rbq_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:numStars="5"
                        android:gravity="center"

                        app:layout_constraintTop_toBottomOf="@id/rq_two"
                        app:layout_constraintStart_toStartOf="@id/rp"
                        app:layout_constraintEnd_toEndOf="@id/rp"
                        android:scaleX=".7"
                        android:scaleY=".7"
                        android:stepSize="1"
                        android:theme="@style/RatingBar"
                        />

                    <TextView
                        android:id="@+id/rq_three"
                        android:text="QUESTION 3"
                        android:gravity="center"

                        app:layout_constraintTop_toBottomOf="@id/rbq_two"
                        app:layout_constraintStart_toStartOf="@id/rp"
                        app:layout_constraintEnd_toEndOf="@id/rp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>

                    <RatingBar
                        android:id="@+id/rbq_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        app:layout_constraintTop_toBottomOf="@id/rq_three"
                        app:layout_constraintStart_toStartOf="@id/rp"
                        app:layout_constraintEnd_toEndOf="@id/rp"
                        android:numStars="5"
                        android:scaleX=".7"
                        android:scaleY=".7"
                        android:gravity="center"

                        android:stepSize="1"
                        android:theme="@style/RatingBar"
                        />

                    <TextView
                        android:id="@+id/thx"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        app:layout_constraintTop_toBottomOf="@id/rbq_three"
                        app:layout_constraintStart_toStartOf="@id/rp"
                        app:layout_constraintEnd_toEndOf="@id/rp"
                        android:text="Thank You!\n Your opinion makes a difference."/>
                    <Button
                        android:id="@+id/finish"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="#149A8D"
                        app:layout_constraintTop_toBottomOf="@id/thx"
                        app:layout_constraintStart_toStartOf="@id/rp"
                        app:layout_constraintEnd_toEndOf="@id/rp"
                        android:text="Finish"
                        android:textAllCaps="false"
                        android:textColor="#fff"
                        tools:layout_editor_absoluteX="161dp"
                        tools:layout_editor_absoluteY="127dp" />



                    <!--        <TextView-->
                    <!--            android:id="@+id/fr_all_ratings_total_tv"-->
                    <!--            android:layout_width="match_parent"-->
                    <!--            android:layout_height="wrap_content"-->
                    <!--            android:layout_marginTop="50dp"-->
                    <!--            android:gravity="center"-->
                    <!--            android:text="all ratings" />-->
                    <!--        <RatingBar-->
                    <!--            android:id="@+id/fr_total_rbq"-->
                    <!--            android:layout_width="wrap_content"-->
                    <!--            android:layout_height="wrap_content"-->
                    <!--            android:layout_gravity="center_horizontal"-->
                    <!--            android:numStars="5"-->
                    <!--            android:stepSize="0.1"-->
                    <!--            android:layout_margin="20dp"-->
                    <!--            android:theme="@style/RatingBar"-->
                    <!--            />-->

                </androidx.constraintlayout.widget.ConstraintLayout>

            </RelativeLayout>

        </ScrollView></RelativeLayout>



        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginTop="-50dp"
            android:src="@drawable/logo_icon"
            android:layout_alignTop="@+id/relativeLayout"
            android:layout_centerHorizontal="true" />

    </RelativeLayout>
</RelativeLayout>
